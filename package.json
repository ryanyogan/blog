{
  "author": "Ryan Yogan <ryanyogan@gmail.com>",
  "name": "ryanyogan-blog",
  "version": "0.0.1",
  "scripts": {
    "start": "node index.js",
    "build-js": "browserify ./app/client/index.js -t reactify | uglifyjs -mc unsafe=true > ./public/js/bundle.js",
    "watch-js": "watchify ./app/client/index.js -t reactify -o ./public/js/bundle.js -dv",
    "build-css": "uglifycss ./public/css/*.css > ./public/css/bundle.css",
    "test": "./node_modules/.bin/mocha ./test/test.js",
    "predeploy": "npm run build-js && npm run build-css",
    "clean": "rm ./public/js/bundle.js"
  },
  "dependencies": {
    "bluebird": "~1.2.0",
    "express": "~3.5.0",
    "fibers": "~1.0.1",
    "highlight.js": "~8.0.0",
    "http-proxy": "~1.0.3",
    "localforage": "^1.2.0",
    "marked": "~0.3.2",
    "node-jsx": "~0.9.0",
    "react": "~0.10.0",
    "react-async": "~0.8.0",
    "react-router-component": "~0.15.0",
    "rss": "~0.3.2",
    "slug": "^0.8.0",
    "superagent": "~0.17.0",
    "yamlhead": "~0.1.0"
  },
  "devDependencies": {
    "connect-browserify": "~2.0.0",
    "reactify": "~0.10.0",
    "mocha": "1.20.1",
    "supertest": "0.13.0"
  }
}
